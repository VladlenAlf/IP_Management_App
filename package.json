{
  "name": "system-zarzadzania-ip",
  "version": "1.0.0",
  "description": "System zarządzania adresami IP z interfejsem w języku polskim",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "network": "HOST=0.0.0.0 node server.js",
    "network:dev": "HOST=0.0.0.0 nodemon server.js",
    "info": "node scripts/network-info.js",
    "docker:build": "docker build -t ip-management-system .",
    "docker:run": "docker run -p 3000:3000 --name ip-management ip-management-system",
    "docker:stop": "docker stop ip-management",
    "docker:remove": "docker rm ip-management",
    "docker:compose:up": "docker compose up -d",
    "docker:compose:down": "docker compose down",
    "docker:compose:logs": "docker compose logs -f",
    "docker:compose:build": "docker compose build --no-cache",
    "docker:compose:restart": "docker compose restart",
    "docker:compose:ps": "docker compose ps"
  },
  "keywords": [
    "ip-management",
    "network",
    "zarządzanie-ip",
    "podsieci",
    "express",
    "sqlite",
    "polish"
  ],
  "author": "System Administrator",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "form-data": "^4.0.4",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "sqlite3": "^5.1.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
